{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "{**/*.ipynb,**/*.csv,**/*.parquet,**/assets/**,**/static/**,**/public/**,**/node_modules/**,**/.venv/**,**/venv/**,**/__pycache__/**}"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/app/$1" }  ]
}
